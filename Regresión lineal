Ybarra = datos_roundup['ID'].mean()
Xbarra = datos_roundup['RU'].mean()

X = datos_roundup[['RU']].values
Y = datos_roundup[['ID']].values

X-Xbarra
Y-Ybarra

b1 = sum((X-Xbarra)*(Y-Ybarra))/ sum((X-Xbarra)*(X-Xbarra))

b0 = Ybarra - b1*Xbarra

